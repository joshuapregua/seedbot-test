{
  "name": "Seedbot",
  "version": "0.1.0",
  "description": "Seedbot application",
  "main": "main/main.js",
  "scripts": {
    "dev": "node scripts/dev-server.js",
    "build": "node scripts/build.js && electron-builder",
    "build:win": "node scripts/build.js && electron-builder --win",
    "build:mac": "node scripts/build.js && electron-builder --mac",
    "build:linux": "node scripts/build.js && electron-builder --linux",
    "lint": "eslint . --ext .ts,.vue",
    "lint:fix": "eslint . --ext .ts,.vue --fix",
    "test": "echo \"Warning: No tests configured yet\" && exit 0",
    "type-check": "tsc --noEmit -p src/main/tsconfig.json && tsc --noEmit -p src/renderer/tsconfig.json"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "@vitejs/plugin-vue": "^4.4.1",
    "autoprefixer": "^10.4.23",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "dotenv": "^17.2.3",
    "electron": "^32.1.2",
    "electron-builder": "^25.1.6",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.29.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vue-eslint-parser": "^9.4.3"
  },
  "dependencies": {
    "vue": "^3.3.8"
  }
}
